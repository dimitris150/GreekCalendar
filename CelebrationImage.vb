            February = 1
        Else
            February = 0
        End If
        'υπολογισμός του πάσχα'
        Dim y2 = Math.Floor(Year / 100)
        Dim e = 10 + y2 - 16 - Math.Floor((y2 - 16) / 4)
        Dim G = Year Mod 19
        Dim I = (19 * G + 15) Mod 30
        Dim J = (Year + Math.Floor(Year / 4) + I) Mod 7
